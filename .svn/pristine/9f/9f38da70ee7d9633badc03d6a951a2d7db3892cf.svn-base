	<!-- 冻结 -->
	<update id="account.updateFreeze" parameterType="com.fang.fangshop.core.bean.Account">
		update t_account
		<set>
			<if test="freeze !=null">
				freeze=#{freeze},
			</if> 
			<if test="freezestartdate !=null">
				freezestartdate=#{freezestartdate},
			</if>
			<if test="freezeenddate !=null">
				freezeenddate=#{freezeenddate}
			</if>
			
		</set>
		where id = #{id}
	</update>

