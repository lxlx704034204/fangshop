	<select id="manage.pay.selectPageList" parameterType="com.fang.fangshop.bean.Pay"
		resultMap="BaseResultMap">
		select id, name, code, seller, order1, status, partner, key1, icon, picture
		from t_pay
		order by Id desc
		limit #{offset},#{pageSize}
