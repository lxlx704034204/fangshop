package com.fang.fangshop.been;

import com.fang.fangshop.core.been.Order;

import java.io.Serializable;
import java.util.List;

public class OrderModel extends Order implements Serializable
{
	private static final long serialVersionUID = 1L;
    //属性
	private List<OrderDetailModel> orderDetails;
	private List<OrderLogModel> orderLogs;
	private List<OrderPayModel> orderPays;
	private OrderShipModel orderShip;
	//查询条件
	
	public List<OrderDetailModel> getOrderDetails() {
		return orderDetails;
	}
	public void setOrderDetails(List<OrderDetailModel> orderDetails) {
		this.orderDetails = orderDetails;
	}
	public List<OrderLogModel> getOrderLogs() {
		return orderLogs;
	}
	public void setOrderLogs(List<OrderLogModel> orderLogs) {
		this.orderLogs = orderLogs;
	}
	public List<OrderPayModel> getOrderPays() {
		return orderPays;
	}
	public void setOrderPays(List<OrderPayModel> orderPays) {
		this.orderPays = orderPays;
	}
	public OrderShipModel getOrderShip() {
		return orderShip;
	}
	public void setOrderShip(OrderShipModel orderShip) {
		this.orderShip = orderShip;
	}
}
